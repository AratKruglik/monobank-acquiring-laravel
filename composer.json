{
    "name": "aratkruglik/monobank-laravel",
    "description": "A comprehensive Monobank API integration for Laravel 11+",
    "keywords": [
        "laravel",
        "monobank",
        "api",
        "banking",
        "finance"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "AratKruglik",
            "email": "author@domain.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AratKruglik\\Monobank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AratKruglik\\Monobank\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AratKruglik\\Monobank\\MonobankServiceProvider"
            ],
            "aliases": {
                "Monobank": "AratKruglik\\Monobank\\Facades\\Monobank"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
